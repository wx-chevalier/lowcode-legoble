{
  "author": "wx-chevalier@github",
  "description": "lego-form-antd",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "name": "lego-form-antd",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/Legoble"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:cjs && npm run copy && npm run build:umd",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:umd": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "copy": "copyfiles -u 1 './src/**/*.css' dist/cjs/",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "pub": "npm run build && npm publish",
    "start": "npm run dev"
  },
  "dependencies": {
    "antd": "^3.21.4",
    "lego-form-core": "^0.1.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-jsonschema-form": "^1.7.0",
    "smoothscroll-polyfill": "^0.4.4",
    "validator.js": "^2.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/jest": "24.0.17",
    "@types/react-dom": "^16.8.5",
    "@types/react-jsonschema-form": "^1.6.2",
    "@types/react-router-dom": "^4.3.4",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/validator": "^10.11.2",
    "autoprefixer": "9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "copyfiles": "^2.1.1",
    "css-loader": "3.2.0",
    "enzyme": "^3.10.0",
    "file-loader": "4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "json-server": "0.15.0",
    "lerna": "^3.16.4",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.12.11",
    "react-router-dom": "^5.0.1",
    "resolve-url-loader": "3.1.0",
    "rimraf": "^2.6.3",
    "style-loader": "1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "^2.1.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-dashboard": "3.0.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "files": [
    "dist/"
  ]
}
